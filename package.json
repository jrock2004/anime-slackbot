{
  "name": "anime-slackbot",
  "version": "1.0.0",
  "description": "A slackbot that allows you to look up your favorite anime",
  "main": "index.js",
  "scripts": {
    "dev": "TOKEN=9999 netlify dev",
    "test": "jest --env=jsdom",
    "test:watch": "jest --env=jsdom --watch",
    "test:coverage": "jest --env=jsdom --coverage",
    "update": "pnpx npm-check-updates -u",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jrock2004/anime-slackbot.git"
  },
  "keywords": [
    "anime",
    "slackbot"
  ],
  "author": "John Costanzo",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jrock2004/anime-slackbot/issues"
  },
  "homepage": "https://github.com/jrock2004/anime-slackbot#readme",
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint"
    ],
    "extends": [
      "plugin:@typescript-eslint/recommended"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.4",
    "@types/node-fetch": "^2.6.11",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^7.13.1",
    "@typescript-eslint/parser": "^7.13.1",
    "eslint": "^9.5.0",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "netlify-cli": "^17.28.0",
    "prettier": "^3.3.2",
    "ts-jest": "^29.1.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "@netlify/functions": "^2.7.0",
    "moment": "^2.30.1",
    "node-fetch": "^3.3.2",
    "string-strip-html": "^13.4.8",
    "uuid": "^10.0.0"
  },
  "engines": {
    "node": ">=14.19.0"
  },
  "volta": {
    "node": "16.20.0",
    "yarn": "1.22.19"
  }
}
